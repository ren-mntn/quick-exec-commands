{
  "name": "quick-exec-commands",
  "displayName": "Quick Execute Commands",
  "description": "Efficiently manage and execute terminal & VS Code commands with a unified interface",
  "version": "1.0.0",
  "icon": "asset/icon.webp",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "command",
    "terminal",
    "productivity",
    "workflow",
    "automation",
    "quick",
    "execute",
    "manage"
  ],
  "publisher": "ren-mntn",
  "repository": {
    "type": "git",
    "url": "https://github.com/ren-mntn/quick-exec-commands.git"
  },
  "homepage": "https://github.com/ren-mntn/quick-exec-commands#readme",
  "bugs": {
    "url": "https://github.com/ren-mntn/quick-exec-commands/issues"
  },
  "license": "MIT",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-command.showPanel",
        "title": "%command.showPanel%"
      },
      {
        "command": "quick-command.showCommandList",
        "title": "%command.showCommandList%"
      },
      {
        "command": "quick-command.addCommand",
        "title": "%command.addCommand%",
        "icon": "$(plus)"
      },
      {
        "command": "quick-command.deleteCommand",
        "title": "%command.deleteCommand%"
      },
      {
        "command": "quick-command.toggleFavorite",
        "title": "%command.toggleFavorite%",
        "icon": "$(star)"
      },
      {
        "command": "quick-command.addDirectory",
        "title": "%command.addDirectory%",
        "icon": "$(new-folder)"
      },
      {
        "command": "quick-command.addCommandToDirectory",
        "title": "%command.addCommandToDirectory%",
        "icon": "$(plus)"
      },
      {
        "command": "quick-command.deleteDirectory",
        "title": "%command.deleteDirectory%"
      },
      {
        "command": "quick-command.toggleDirectoryExpansion",
        "title": "%command.toggleDirectoryExpansion%"
      },
      {
        "command": "quick-command.editCommand",
        "title": "%command.editCommand%"
      },
      {
        "command": "quick-command.exportCommands",
        "title": "%command.exportCommands%"
      },
      {
        "command": "quick-command.importCommands",
        "title": "%command.importCommands%"
      },
      {
        "command": "quick-command.searchCommands",
        "title": "%command.searchCommands%",
        "icon": "$(search)"
      },
      {
        "command": "quick-command.showHistory",
        "title": "%command.showHistory%"
      },
      {
        "command": "quick-command.debugTreeView",
        "title": "%command.debugTreeView%"
      },
      {
        "command": "quick-command.createSampleCommands",
        "title": "%command.createSampleCommands%"
      }
    ],
    "keybindings": [
      {
        "command": "quick-command.showCommandList",
        "key": "ctrl+shift+x",
        "mac": "cmd+shift+x"
      },
      {
        "command": "quick-command.searchCommands",
        "key": "ctrl+shift+f",
        "mac": "cmd+shift+f"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "quick-command",
          "title": "%viewContainer.quickCommand%",
          "icon": "$(terminal)"
        }
      ]
    },
    "views": {
      "quick-command": [
        {
          "id": "quickCommandPanel",
          "name": "%view.quickCommandPanel%",
          "when": "true"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "quickCommandPanel",
        "contents": "%viewWelcome.content%",
        "when": "view == quickCommandPanel"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "quick-command.deleteCommand",
          "when": "false"
        },
        {
          "command": "quick-command.toggleFavorite",
          "when": "false"
        },
        {
          "command": "quick-command.addCommandToDirectory",
          "when": "false"
        },
        {
          "command": "quick-command.deleteDirectory",
          "when": "false"
        },
        {
          "command": "quick-command.toggleDirectoryExpansion",
          "when": "false"
        },
        {
          "command": "quick-command.editCommand",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "quick-command.addCommand",
          "when": "view == quickCommandPanel",
          "group": "navigation@1"
        },
        {
          "command": "quick-command.addDirectory",
          "when": "view == quickCommandPanel",
          "group": "navigation@2"
        },
        {
          "command": "quick-command.searchCommands",
          "when": "view == quickCommandPanel",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "quick-command.toggleFavorite",
          "when": "view == quickCommandPanel && viewItem == quickCommand",
          "group": "inline@1"
        },
        {
          "command": "quick-command.addCommandToDirectory",
          "when": "view == quickCommandPanel && viewItem =~ /.*Category$/",
          "group": "inline@1"
        },
        {
          "command": "quick-command.addDirectory",
          "when": "view == quickCommandPanel && viewItem =~ /.*Category$/",
          "group": "inline@2"
        },
        {
          "command": "quick-command.addCommandToDirectory",
          "when": "view == quickCommandPanel && viewItem == commandDirectory",
          "group": "inline@1"
        },
        {
          "command": "quick-command.editCommand",
          "when": "view == quickCommandPanel && viewItem == quickCommand",
          "group": "command@1"
        },
        {
          "command": "quick-command.deleteCommand",
          "when": "view == quickCommandPanel && viewItem == quickCommand",
          "group": "command@2"
        },
        {
          "command": "quick-command.deleteDirectory",
          "when": "view == quickCommandPanel && viewItem == commandDirectory",
          "group": "command@1"
        },
        {
          "command": "quick-command.exportCommands",
          "when": "view == quickCommandPanel && viewItem =~ /.*Category$/",
          "group": "export@1"
        },
        {
          "command": "quick-command.importCommands",
          "when": "view == quickCommandPanel && viewItem =~ /.*Category$/",
          "group": "export@2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "16.x",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "css-loader": "^6.7.0",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.0",
    "prettier": "^2.8.0",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.4.0",
    "typescript": "^4.9.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
